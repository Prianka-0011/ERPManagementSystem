@model IEnumerable<ERPManagementSystem.Models.City>
@using ERPManagementSystem.Extensions;

@{
    ViewData["Title"] = "city";
}
<div class="action">
    <div class="row">
        <div class="col-md-3 col-12 col-lg-3">
            <a class="create" onclick="ShowInSmallPopup('@Url.Action("AddOrEdit","Cities",null,Context.Request.Scheme)','New City' )"><i class="fas fa-plus"></i><span>New</span></a>
        </div>
    </div>
</div>
<div class="body">
    <div class="row">
        <div class="col-md-12 col-12 col-lg-12">
            <div id="view-all">
                @await Html.PartialAsync("_ViewAllCity", Model)

            </div>
        </div>
    </div>
</div>


