@model IEnumerable<ERPManagementSystem.Models.Quotation>
@{
    ViewData["Title"] = "Quotation";
}


<div class="action">
    <div class="row">
        <div class="col-md-3 col-12 col-lg-3">
            <a class="create" onclick="ShowInLargePopup('@Url.Action("AddOrEdit","Quotations",null,Context.Request.Scheme)','New Quotation' )"><i class="fas fa-plus"></i><span>New</span></a>
            <p class="recordcount">@Model.Count() record exist</p>
        </div>
    </div>
</div>
<div class="body">
    <div class="row">
        <div class="col-md-12 col-12 col-lg-12">
            <div id="view-all">
                @await Html.PartialAsync("_ViewAllQuotation", Model)
            </div>
        </div>
    </div>
</div>
